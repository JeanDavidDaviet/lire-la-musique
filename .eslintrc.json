{
  "extends": ["react-app", "react-app/jest"],
  "rules": {
    "jsx-a11y/anchor-is-valid": "warn",
    "no-unused-vars": "warn",
    "no-console": [
      "warn",
      {
        "allow": ["warn", "error"]
      }
    ],
    "eqeqeq": "warn",
    "no-var": "error",
    "prefer-const": "warn"
  },
  "overrides": [
    {
      "files": ["**/*.test.js", "**/*.spec.js"],
      "env": {
        "jest": true
      }
    }
  ]
}
